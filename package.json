{
  "name": "@dSafe/sdk",
  "version": "0.0.1",
  "description": "A decentralised registry for Safe (Safe-Global) to replacce Safe's Safe Transaction API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": "git@github.com:daoism-systems/dSafe-SDK.git",
  "author": "Mihirsinh Parmar <mihirsinh.parmar.it@gmail.com>",
  "scripts": {
    "lint": "eslint src/**/*.{js,ts,json}",
    "lint:fix": "eslint --fix 'src/**/*.{js,ts,json}'",
    "format": "prettier --write 'src/**/*.{js,ts,css,md,json}' --config ./.prettierrc",
    "build": "tsup",
    "publish": "bash shell/publish.sh",
    "publish:npm": "yarn npm publish --access public",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/**/*.ts'",
    "coverage": "nyc yarn test"
  },
  "engines": {
    "node": ">=18.18.2"
  },
  "keywords": [
    "gnosis safe",
    "decentralised safe registry",
    "dsafe sdk"
  ],
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.9",
    "@types/eslint": "^8",
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.8.9",
    "@types/sinon": "^10.0.20",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.9.0",
    "chai": "^4.3.10",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^3.0.3",
    "sinon": "^17.0.0",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@4.0.1"
}
